<Window x:Class="EX6.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EX6"
        mc:Ignorable="d"
        Title="MainWindow" Height="400" Width="400">
    <Grid>
        <TextBox x:Name="DataTextBox" Margin="10 10 10 0" VerticalAlignment="Top" Height="200" Text="2 3 3&#xA;2 3 9&#xA;5 7 3"/>
        <Button VerticalAlignment="Top" HorizontalAlignment="Left" Width="50" Margin="10 215 10 10" Content="\n" Click="PressEnter"/>
        <Button VerticalAlignment="Bottom" Margin="10 0 10 10" Content="OK" Click="ProceedTask"/>
    </Grid>
</Window>
