<Window x:Class="EX1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EX1"
        mc:Ignorable="d"
        Title="MainWindow" Height="130" Width="345">
    <Grid>
        <TextBox x:Name="PathTextBox" VerticalAlignment="Top" HorizontalAlignment="Left" Width="300" IsEnabled="False" FontSize="11" Margin="10 10 0 0"/>
        <Button VerticalAlignment="Top" HorizontalAlignment="Left" Width="300" Height="20" Content="Выбор файла" 
                FontSize="11" Margin="10 35 0 0" Click="SelectFileAction"/>
        <Button x:Name="PrintDataButton" VerticalAlignment="Top" HorizontalAlignment="Left" Width="300" Height="20" Content="Вывести числа" 
                FontSize="11" IsEnabled="False" Margin="10 60 0 0" Click="PrintDataAction"/>
    </Grid>
</Window>
